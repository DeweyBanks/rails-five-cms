<h1><%= params[:filter].upcase %></h1>

<table>
  <thead>
    <tr>
      <th>Avatar</th>
      <th>Title</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>About</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @commanders.each do |cm| %>
      <tr>
        <td><%=  %></td>
        <td><%= cm.title %></td>
        <td><%= cm.first_name %></td>
        <td><%= cm.last_name %></td>
        <td><%= cm.about %></td>
      </tr>
    <% end %>
  </tbody>
</table>

